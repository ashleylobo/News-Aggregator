{
  "name": "newsaggregator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare-repository": "npm i -g react-native-cli yarn;yarn install; react- native link",
    "emulator": "emulator -avd Nexus5V6L23_x86_64 -scale 1.0",
